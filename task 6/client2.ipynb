    "import socket\n",
    "\n",
    "HOST='127.0.0.1'\n",
    "PORT=56\n",
    "c= socket.socket (socket.AF_INET, socket.SOCK_STREAM)\n",
    "c.connect((HOST,PORT))\n",
    "file_name=input(\"Enter the file name: \")\n",
    "c.sendall(file_name.encode())\n",
    "file_data = c.recv(1024)\n",
    "print(file_data.decode())\n",
    "print(\"file received succesfully\")\n",
    "c.close()"
   ]
  }
