   "source": [
    "import socket\n",
    "\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)\n",
    "\n",
    "file_name = input(\"Enter the name of the binary file you want to request: \")\n",
    "s.sendto(file_name.encode(), (\"localhost\", 59))\n",
    "\n",
    "data, addr = s.recvfrom(1024)\n",
    "print(data.decode())\n",
    "\n",
    "s.close()"
   ]
  }
